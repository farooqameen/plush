// Test .min() and .max() methods for Int64 and Float64

// Test Int64.min()
assert((5).min(10) == 5);
assert((10).min(5) == 5);
assert((-5).min(-10) == -10);
assert((5).min(5) == 5);

// Test Int64.max()
assert((5).max(10) == 10);
assert((10).max(5) == 10);
assert((-5).max(-10) == -5);
assert((5).max(5) == 5);

// Test Float64.min()
assert((5.5).min(10.5) == 5.5);
assert((10.5).min(5.5) == 5.5);
assert((-5.5).min(-10.5) == -10.5);
assert((5.5).min(5.5) == 5.5);

// Test Float64.max()
assert((5.5).max(10.5) == 10.5);
assert((10.5).max(5.5) == 10.5);
assert((-5.5).max(-10.5) == -5.5);
assert((5.5).max(5.5) == 5.5);

$println("min/max tests passed!");